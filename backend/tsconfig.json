{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "backend/src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["backend/src/**/*"],
  "exclude": ["node_modules"]
}